{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "i18next-extract",
      {
        "locales": [
          "en",
          "it"
        ],
        "discardOldKeys": true,
        "outputPath": "public/i18n/{{locale}}.json"
      }
    ]
  ]
}